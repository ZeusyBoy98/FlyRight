{
  "expo": {
    "name": "FlyRight",
    "slug": "flyright",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/FlyRight.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.zeusyboy.flyrightapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/FlyRight.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zeusyboy.flyright"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          // "resizeMode": "cover",
          // "imageWidth": 200,
          // "image": "./assets/images/FlyRight.png",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3f691559-81c2-460e-9ee2-ffb26a6a1217"
      }
    },
    "owner": "zeusyboy"
  },
  "build" : {
    "production" : {
      "android" : {
        "buildType" : "apk"
      }
    }
  }
}
